{
  "extends": [ "airbnb", "prettier", "prettier/react", "plugin:jest/recommended" ],
  "plugins": [ "prettier", "jest" ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}